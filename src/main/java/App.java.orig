package com.timetrade.lamdatest;

import java.util.Map;
import com.amazonaws.services.lambda.runtime.Context; 
import com.amazonaws.services.lambda.runtime.RequestHandler;

/*
 * This Java source file was generated by the Gradle 'init' task.
 */
public class App implements RequestHandler<Map<String, Object>, Integer>{

	public Integer handleRequest(Map<String, Object> input, Context context){
		int countA = (Integer)input.get("valA");
		int countB = (Integer)input.get("valB");
		return new Integer(countA+countB);
	}

	public App(){}

	public static void main(String args[]){

	}	

}
